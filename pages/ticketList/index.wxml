<!--pages/ticketList/index.wxml-->
<view class="container page-ticketlist">
  <view class="ticket-list"  wx:if="{{ticketInfo.length >0}}">
    <view class="ticket__item" wx:for="{{ticketInfo}}" wx:key>
        <i-ticket ticketInfo="{{item}}"></i-ticket>
    </view>
  </view>
  <i-nodata wx:if="{{ticketInfo.length == 0}}" content="暂无优惠券信息"></i-nodata>
</view>