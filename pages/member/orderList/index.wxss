page{
  background-color: #fff;
}
.row,.price-bar{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.row,.row1{
  padding: 20rpx 24rpx;
}

.f-info{
  position: relative;
  height: 200rpx;
}
.price-bar{
  position: absolute;
  width: 100%;
  left: 0;
  bottom: 0;
  box-sizing: border-box;
  padding-left: 220rpx;
}
.integral-list .f-item{
  border-bottom:10rpx solid #f4f4f4;
  /* padding: 0 24rpx; */
}
.integral-list .f-image image{
  width: 200rpx;
  height: 200rpx;
  border-radius: 36rpx;

}
.integral-list .f-image{
  float: left;
  margin-right:20rpx;
}

.border-top{
  border-top:2rpx dashed #f4f4f4;
}
.f-name{
  font-size: 28rpx;
}