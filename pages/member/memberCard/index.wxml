<!--pages/member/memberCard/index.wxml-->
<view class="container page-member-card">
  <view class="member-card">
    <image src="/images/card.png"></image>
    <view class="card__text text-white">
      <view class="card__name">
        十点书店会员卡
      </view>
      <view class="card__level">
        <view>
          {{userInfo.card_name}}
        </view>
        <view>
          {{userInfo.login_name}}
        </view>
      </view>
    </view>
  </view>
  <view class="member-card-content">
    <view class="tit">会员权益</view>
    <view class="con">
      <image src="http://res.sdibook.com/DefaultRes/Images/quanyi.jpeg" style="height:850px;"></image>
    </view>
  </view>
</view>
<i-bottombar></i-bottombar>